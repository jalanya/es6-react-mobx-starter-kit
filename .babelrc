{
  "presets": [
    ["env", {
     "targets": {
       "node": "current"
     }
    }],
    "react",
    "stage-0"
  ],
  "plugins": [
    "transform-es2015-destructuring",
    "transform-es2015-parameters",
    "transform-object-rest-spread",
    "transform-runtime",
    "add-module-exports",
    "transform-decorators-legacy",
    "transform-react-display-name"
  ],
  "ignore": [
    "node_modules/**/*.js"
  ]
}
